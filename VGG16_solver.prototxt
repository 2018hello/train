# class = 344, n=90248, batch=64, 90248/64 = 1410次iter完成一个epoch. Test:38679/50 = 774
net: "VGG16_Face_train_val.prototxt"
test_iter: 200
test_interval: 1000
base_lr: 0.001
lr_policy: "step"
gamma: 0.1
stepsize: 7000
display: 1
max_iter: 30000
momentum: 0.9
weight_decay: 0.0005
snapshot: 1000
snapshot_prefix: "VGG_Face_finetune"
solver_mode: GPU
